{
    "name": "svoboda/psr-router",
    "description": "Simple router built on top of PSR-7.",
    "keywords": ["router", "routing", "http", "psr", "psr-7"],
    "homepage": "https://github.com/jansvoboda11/psr-router",
    "license": "MIT",
    "authors": [
        {
            "name": "Jan Svoboda",
            "email": "jansvoboda11@gmail.com",
            "homepage": "https://github.com/jansvoboda11",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpbench/phpbench": "^0.14.0",
        "phpstan/phpstan": "^0.10.0",
        "phpunit/phpunit": "^7.2",
        "zendframework/zend-diactoros": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "Svoboda\\PsrRouter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SvobodaTest\\PsrRouter\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "phpstan analyse src/ --level=7",
        "bench": "phpbench run benchmarks/ --report=aggregate",
        "test": "phpunit tests/"
    }
}
