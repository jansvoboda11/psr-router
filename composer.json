{
    "name": "svoboda/router",
    "description": "Router is a routing library built with PSR-7 and PSR-15 in mind.",
    "keywords": ["router", "routing", "http", "psr", "psr-7", "psr-15"],
    "homepage": "https://github.com/jansvoboda11/router",
    "license": "MIT",
    "authors": [
        {
            "name": "Jan Svoboda",
            "email": "jansvoboda11@gmail.com",
            "homepage": "https://github.com/jansvoboda11",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "psr/http-factory": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.8",
        "nyholm/psr7": "^1.0",
        "phpbench/phpbench": "^0.14.0",
        "phpstan/phpstan": "^0.10.0",
        "phpunit/phpunit": "^7.3",
        "svoboda/dispatcher": "^0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Svoboda\\Router\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SvobodaTest\\Router\\": "tests/"
        }
    }
}
